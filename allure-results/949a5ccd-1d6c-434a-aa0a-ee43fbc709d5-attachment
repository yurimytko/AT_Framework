  <section class="form--section">
  <h3 class="form--section-title">Preferences</h3>
  
  <div class="form--field">
  <label class="form--label" title="Time zone" for="pref_time_zone">Time zone</label><span class=" form--field-container"><span class="form--select-container -middle"><select class="form--select" name="pref[time_zone]" id="pref_time_zone"><option value="" label=" "></option>
